{
  "name": "mingkwai-typesetter",
  "version": "0.2.0",
  "description": "A streaming MarkDown -> (Xe, Lua)LaTeX -> PDF typesetter",
  "main": "lib/main.js",
  "scripts": {
    "mkts": "node --harmony lib/cli.js mkts $2",
    "build": "coffee --map -o lib -c src && coffee --map -o lib -c options.coffee",
    "push": "git add . && git commit -m'update' && git push",
    "doctoc": "doctoc README.md README-plugin-tabulate.md README-plugin-tsv.md",
    "pub": "npm run build && npm run doctoc && npm run push && npm version patch && npm publish",
    "test-travis": "node lib/tests.js",
    "test": "npm run build && node lib/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loveencounterflow/mingkwai-typesetter.git"
  },
  "keywords": [
    "mingkwai",
    "typesetter",
    "jizura",
    "tex",
    "latex",
    "luatex",
    "xetex",
    "markdown"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loveencounterflow/mingkwai-typesetter/issues"
  },
  "homepage": "https://github.com/loveencounterflow/mingkwai-typesetter",
  "dependencies": {
    "ajv": "^6.5.5",
    "async": "^2.6.1",
    "changeset": "^0.1.0",
    "chinesegen": "^0.3.3",
    "cnd": "^4.3.1",
    "coffeenode-chr": "^1.0.4",
    "coffeenode-suspend": "^0.1.11",
    "coffeescript": "^2.3.2",
    "commander": "^2.19.0",
    "cxltx-styles": "^1.4.7",
    "font-cmap": "^1.0.1",
    "htmlparser2": "^3.10.0",
    "hyphenation.en-us": "^0.2.1",
    "hypher": "^0.2.4",
    "intergrid": "^0.4.1",
    "linebreak": "^0.3.0",
    "lorem-ipsum": "^1.0.6",
    "lorem-ipsum-japanese": "^1.0.1",
    "markdown-it": "^5.0.2",
    "markdown-it-container": "^2.0.0",
    "markdown-it-footnote": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-multimd-table": "^2.0.1",
    "markdown-it-regexp": "^0.4.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "minimatch": "^3.0.4",
    "ncr": "^0.2.12",
    "parse5": "^5.1.0",
    "pipedreams": "^3.0.9",
    "pipestreams": "^0.4.1",
    "rehype-stringify": "^4.0.0",
    "remark-grid-tables": "^1.2.0",
    "remark-html": "^8.0.0",
    "remark-parse": "^5.0.0",
    "remark-rehype": "^3.0.1",
    "semver": "^5.6.0",
    "sqy": "^0.1.1",
    "unified": "^7.0.2",
    "xregexp": "^4.2.0"
  },
  "devDependencies": {
    "guy-test": "^1.3.3"
  }
}
