    "jizura",
    "tex",
    "latex",
    "luatex",
    "xetex",
    "markdown"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loveencounterflow/mingkwai-typesetter/issues"
  },
  "homepage": "https://github.com/loveencounterflow/mingkwai-typesetter",
  "dependencies": {
    "async": "^1.5.0",
    "cnd": "^1.0.9",
    "coffee-script": "^1.10.0",
    "coffeenode-chr": "^1.0.2",
    "coffeenode-suspend": "^0.1.11",
    "coffeenode-text": "^0.1.9",
    "commander": "^2.9.0",
    "cxltx-styles": "^1.4.3",
    "font-cmap": "^1.0.1",
    "htmlparser2": "^3.9.0",
    "hyphenation.en-us": "^0.2.1",
